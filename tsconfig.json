{
	"compilerOptions": {
		"baseUrl": "src",
		"paths": {
			"@lib/*": ["lib/*"],
			"@model/*": ["model/*"]
		},
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "es6",
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"resolveJsonModule": true
	},
	"lib": ["es2015"],
	"include": ["src", "knex"],
	"exclude": ["node_modules"]
}
