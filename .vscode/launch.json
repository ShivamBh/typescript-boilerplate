{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "Debug main.ts",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceRoot}",
			"runtimeArgs": [
				"-r",
				"${workspaceRoot}/node_modules/ts-node/register",
				"-r",
				"${workspaceRoot}/node_modules/tsconfig-paths/register"
			],
			"env": {
				"NODE_ENV": "development",
				"TS_NODE_PROJECT": "${workspaceRoot}/tsconfig.json"
			},
			"args": ["${workspaceRoot}/src/index.ts"]
		}
	]
}
